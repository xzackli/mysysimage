[
    {
        "key": "shift+enter",
        "command": "language-julia.executeCellAndMove",
        "when": "editorTextFocus && !findInputFocussed && !jupyter.ownsSelection && !notebookEditorFocused && !replaceInputFocussed && editorLangId == 'julia'"
    },
    {
        "key": "shift+enter",
        "command": "-language-julia.executeCellAndMove",
        "when": "editorTextFocus && editorLangId == 'julia'"
    }
]
